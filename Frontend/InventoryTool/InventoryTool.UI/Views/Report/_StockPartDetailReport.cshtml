
@model IEnumerable<InventoryTool.Model.GetStockPartDetailReport_Result>
@{
    ViewBag.Title = "Products";
}
<script src="~/Scripts/Product/Product.js"></script>
<link href="@Url.Content("~/Content/themes/base/jquery-ui.css")" rel="Stylesheet" type="text/css">
<link href="~/Content/custom-css/custome.css" rel="stylesheet" />
<link href="~/Content/custom-css/shared.css" rel="stylesheet" />
<div class="container" style="margin-top:23px">
    <div id="dvMinBalanceReport">
        <table style="width:100%;" cellspacing="0" class="table table-bordred" id="tblFilterList">
            <thead>
                <tr>                  
                    <th style="text-align:left">Name</th>
                    @*<th style="text-align:left">Type</th>
                    <th style="text-align:left">Outsource</th>
                    <th style="text-align:left">Category</th>
                    <th style="text-align:left">Color</th>
                    <th style="text-align:left">Material</th>
                    <th style="text-align:left">Criticality</th>
                    <th style="text-align:left">Unit</th>
                    <th style="text-align:left">Min Quantity</th>
                    <th style="text-align:left">Balance</th>*@
                </tr>
            </thead>
            <tbody>
                @if (Model != null)
                {
                    foreach (var data in Model)
                    {
                        <tr data-Part_Id="@data.Part_Id" >                           
                            <td title="@data.Part_Name">
                                @data.Part_Name
                            </td>
                            @*<td title="@data.Part_Type">
                                @data.Part_Type
                            </td>
                            <td title="@data.Outsource_Type">
                                @data.Outsource_Type
                            </td>
                            <td title="@data.Category_Name">
                                @data.Category_Name
                            </td>
                            <td title="@data.Color_Name">
                                @data.Color_Name
                            </td>
                            <td title="@data.Material_Name">
                                @data.Material_Name
                            </td>
                            <td title="@data.Criticality_Name">
                                @data.Criticality_Name
                            </td>
                            <td title="@data.Unit_Name">
                                @data.Unit_Name
                            </td>
                            <td title="@data.Min_Quantity">
                                @data.Min_Quantity
                            </td>
                            <td title="@data.Balance_Quantity">
                                @data.Balance_Quantity
                            </td>*@
                        </tr>
                    }
                }
            </tbody>
        </table>

    </div>
</div>





